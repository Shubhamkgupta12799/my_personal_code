<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-4.3.xsd
    http://www.springframework.org/schema/context
    http://www.springframework.org/schema/context/spring-context-4.3.xsd">


	<bean id="itemBeans1" class="com.zensar.Item">
		<property name="name" value="Laptop" />
		<property name="price" value="7000" />
	</bean>
	
	<bean id="itemBeans2" class="com.zensar.Item">
		<property name="name" value="Desktop" />
		<property name="price" value="12000" />
	</bean>

	<bean id="orderBeans1" class="com.zensar.Order">
		<property name="item" ref="itemBeans1" />
		<property name="title" value="Laptop purchase" />
	</bean>
	
	
	<bean id="orderBeans2" class="com.zensar.Order">
		<property name="item" ref="itemBeans2" />
		<property name="title" value="Desktop purchase" />
	</bean>
</beans>